/*每个页面公共css */
page {
  background: #f5fafd;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  font-family: Roboto-Black;
  min-height: 100vh;
}
.content {
  width: 100%;
  margin: 0 12px;
}
.bg {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  margin-top: 168rpx;
}
.nav_heads {
  position: fixed;
  z-index: 999;
  top: 0rpx;
  left: 0;
  height: 168rpx;
  width: 100%;
  background: url("http://pic.doggieye.com/20191218/7a8f469a32254c6aac96a996ede18d45.png");
  background-size: 100%;
  text-align: center;
  font-size: 32rpx;
  font-family: Source Han Sans CN;
  color: #333333;
  font-weight: 600;
}
.nav_heads text:nth-of-type(1) {
    width: 100rpx;
    height: 168rpx;
    display: inline-block;
    text-align: center;
    position: absolute;
    left: 0rpx;
    top: 0rpx;
}
.nav_heads text:nth-of-type(2) {
    max-width: 360rpx;
    height: 100%;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    display: inline-block;
    text-align: center;
}

