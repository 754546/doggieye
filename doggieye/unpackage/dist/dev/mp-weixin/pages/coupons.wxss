  width: 96rpx;
  height: 44rpx;
  background: rgba(255, 255, 255, 0.6);
  border: 2rpx solid #1d201f;
  border-radius: 22rpx;
  font-size: 22rpx;
  line-height: 44rpx;
  text-align: center;
  position: absolute;
  right: 10rpx;
  top: 78rpx;
}
.active {
  color: #333 !important;
  font-size: 28rpx;
  font-weight: 600;
}
.nav {
  width: 100%;
  line-height: 80rpx;
  height: 80rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.nav view {
    width: 33.33%;
    line-height: 80rpx;
    height: 80rpx;
    font-size: 26rpx;
    text-align: center;
    position: relative;
    color: #999999;
}
.popup {
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, 0.5);
  position: fixed;
  z-index: 999999;
  top: 0;
  left: 0;
}
.popup .popupInfo {
    position: relative;
    top: 500rpx;
    left: 120rpx;
    width: 510rpx;
    height: 274rpx;
    background: white;
    border-radius: 20rpx;
    background: #FFFFFF;
}
.popup .popupInfo view:nth-of-type(1) {
      padding: 20rpx 0;
      text-align: center;
      font-size: 30rpx;
}
.popup .popupInfo view:nth-of-type(2) {
      padding: 20rpx 0;
      text-align: center;
      font-size: 32rpx;
      font-weight: 600;
}
.popup .popupInfo view:nth-of-type(3) {
      margin-top: 22rpx;
      font-size: 30rpx;
      border-top: 1rpx solid #F8F8F8;
}
.popup .popupInfo view:nth-of-type(3) text {
        display: inline-block;
        box-sizing: border-box;
        width: 50%;
        text-align: center;
        color: #5D9DDE;
        height: 88rpx;
        line-height: 88rpx;
}
.popup .popupInfo view:nth-of-type(3) text:first-child {
        border-radius: 8rpx;
        border-right: 1rpx solid #F8F8F8;
}
.popup .popupInfo view:nth-of-type(3) text:last-child {
        border-radius: 8rpx;
        font-weight: 600;
}

