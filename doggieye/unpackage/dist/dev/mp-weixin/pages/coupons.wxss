.coupons {
  position: relative;
  box-shadow: 1rpx 2rpx 15rpx 0rpx rgba(145, 165, 179, 0.2);
  margin: 30rpx 0 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 100%;
  height: 180rpx;
  background: #FFFFFF;
}
.coupons .quota {
    font-size: 80rpx;
    color: #fff;
    font-weight: bold;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
            justify-content: center;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    background: url("http://pic.doggieye.com/20191217/ad1a2b66428943ccb8189e6a6cf44878.png");
    background-size: 100%;
    width: 224rpx;
    height: 180rpx;
}
.coupons .introduction {
    color: #999999;
    margin-left: 20rpx;
}
.coupons .introduction view:first-child {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
              justify-content: space-between;
}
.coupons .introduction view:first-child image {
        display: inline-block;
        width: 96rpx;
        height: 116rpx;
        position: absolute;
        top: 42rpx;
        right: 0;
}
.coupons .introduction view {
      line-height: 80rpx;
}
