page {
  background: #E4F2FA;
}
.info {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 100%;
  background: white;
  box-shadow: 1rpx 2rpx 15rpx 0rpx rgba(145, 165, 179, 0.2);
  border-radius: 10rpx;
  margin: 20rpx 26rpx 0;
}
.info image {
    width: 361rpx;
    height: 117rpx;
    padding: 32rpx 0 60rpx;
}
.info .title {
    width: 100%;
    margin: 0 26rpx;
    line-height: 88rpx;
    height: 88rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    font-size: 28rpx;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
            justify-content: space-between;
}
.info .title .iconfont {
      font-size: 20rpx;
}
.logo {
  width: 600rpx;
  display: block;
  margin: 200rpx auto;
}
.body {
  width: 100%;
  position: relative;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.version {
  font-size: 20rpx;
  text-align: center;
  line-height: 30rpx;
  height: 60rpx;
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 72rpx;
  font-family: PingFang SC;
  color: #999999;
}

